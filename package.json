{
  "name": "template-workspace",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o --port=8103 --host=0.0.0.0",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "ci": "ng lint && ng test && ng build",
    "format:test": "prettier --list-different \"./projects/**/*.{ts,html,scss,json}\"",
    "format:write": "prettier --write \"./projects/**/*.{ts,html,scss,json}\"",
    "analyze": "ng build --stats-json --output-hashing=none --named-chunks && esbuild-visualizer --template treemap --metadata dist/template/stats.json --filename dist/template/analyse/index.html  && http-server -o -c-1 ./dist/template/analyse/",
    "analyze:sme": "ng build --source-map --output-hashing=none --named-chunks && source-map-explorer dist/template/browser/*.js --html dist/template/sme/index.html && http-server -o -c-1 ./dist/template/sme/",
    "analyze:deps": "npm-run-all analyze:deps:*",
    "analyze:deps:all": "madge projects/template/src/main.ts --ts-config tsconfig.json --image ./deps/template/_all.png",
    "analyze:deps:ui": "madge  --extensions ts --ts-config tsconfig.json --image ./deps/template/ui.png projects/template/src/app/ui/",
    "analyze:deps:pattern": "madge  --extensions ts --ts-config tsconfig.json --image ./deps/template/pattern.png projects/template/src/app/pattern/",
    "analyze:deps:feature": "madge  --extensions ts --ts-config tsconfig.json --image ./deps/template/feature.png projects/template/src/app/feature/",
    "analyze:deps:layout": "madge  --extensions ts --ts-config tsconfig.json --image ./deps/template/layout.png projects/template/src/app/layout/",
    "analyze:deps:core": "madge  --extensions ts --ts-config tsconfig.json --image ./deps/template/core.png projects/template/src/app/core/"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.0.4",
    "@angular/cdk": "^18.0.4",
    "@angular/common": "^18.0.4",
    "@angular/compiler": "^18.0.4",
    "@angular/core": "^18.0.4",
    "@angular/forms": "^18.0.4",
    "@angular/platform-browser": "^18.0.4",
    "@angular/platform-browser-dynamic": "^18.0.4",
    "@angular/router": "^18.0.4",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.0.5",
    "@angular-eslint/builder": "18.3.0",
    "@angular-eslint/eslint-plugin": "18.3.0",
    "@angular-eslint/eslint-plugin-template": "18.3.0",
    "@angular-eslint/schematics": "18.3.0",
    "@angular-eslint/template-parser": "18.3.0",
    "@angular/cli": "^18.0.5",
    "@angular/compiler-cli": "^18.0.4",
    "@types/jasmine": "~5.1.0",
    "@typescript-eslint/eslint-plugin": "7.8.0",
    "@typescript-eslint/parser": "7.8.0",
    "autoprefixer": "^10.4.19",
    "esbuild-visualizer": "^0.6.0",
    "eslint": "^8.57.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-boundaries": "^4.2.0",
    "http-server": "^14.1.1",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "madge": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "source-map-explorer": "^2.5.3",
    "typescript": "~5.4.2"
  }
}
